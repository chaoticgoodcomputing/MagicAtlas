{
  "Flowthru": {
    "Catalog": {
      "Type": "MagicAtlas.Data.Catalog",
      "ConstructorArgs": {
        "basePath": "Data"
      }
    },
    "Pipelines": {
      "CardProcessing": {
        "FilterOptions": {
          "Format": "commander",
          "Colors": null,
          "ColorIdentity": null,
          "Types": null,
          "Rarities": null,
          "Sets": null,
          "MinCmc": null,
          "MaxCmc": null,
          "ExcludeDigital": true,
          "ExcludePromo": true,
          "ExcludeUnreleased": false
        }
      },
      "EmbeddingAnalytics": {
        "NearestNeighborOptions": {
          "NeighborCount": 5,
          "ExcludedTextTypes": [ "KeywordAbility" ],
          "TargetCardNames": [
            "Sol Ring",
            "Chatterfang, Squirrel General",
            "Wrath of God",
            "Counterspell",
            "Lightning Bolt",
            "Ravages of War"
          ],
          "RandomSampling": {
            "Seed": null,
            "TargetCount": 20
          }
        },
        "EmbeddingSamplingParams": {
          "SampleCount": 10,
          "Seed": 42
        },
        "DistributionChartParams": {
          "BinCount": 8
        },
        "ClusteringParams": {
          "NumberOfClusters": 64,
          "Seed": 42
        },
        "EnrichmentParams": {
          "TopN": 3
        }
      },
      "EmbeddingReductions": {
        "PcaOptions": {
          "Rank": 50,
          "Seed": 42,
          "EnsureZeroMean": true
        },
        "ScatterPlotOptions": {
          "ExcludedTextTypes": [ "Full" ]
        }
      },
      "EmbeddingViz": {
        "ScatterPlotByCmcOptions": {
          "SizingMetric": "Cmc",
          "SizeMultiplier": 0.5,
          "MinimumMarkerSize": 5.0,
          "MarkerOpacity": 0.6,
          "ExcludedTextTypes": [ "Full" ],
          "RandomSeed": 42
        },
        "ScatterPlotByPriceOptions": {
          "SizingMetric": "Price",
          "SizeMultiplier": 0.1,
          "MinimumMarkerSize": 5.0,
          "MarkerOpacity": 0.6,
          "ExcludedTextTypes": [ "Full" ],
          "RandomSeed": 42
        }
      },
      "UmapExploration": {
        "FilterSampleOptions": {
          "ExcludedTextTypes": [ "Full" ],
          "SamplePercentage": 0.5,
          "RandomSeed": 42
        },
        "ScanOptions": {
          "NumberOfNeighborsValues": [ 2, 4, 6, 8 ],
          "MinDistValues": [ 0.25, 0.5, 0.75 ],
          "NumberOfComponents": 2,
          "Metric": "euclidean",
          "NumberOfEpochs": 100,
          "Seed": 42,
          "Verbosity": 0,
          "NoiseScale": 0.0001
        },
        "VisualizationOptions": {
          "Width": 2560,
          "Height": 2560,
          "MarkerSize": 3,
          "MarkerOpacity": 0.3
        }
      },
      "MagicAstTesting": {
        "SampleSize": 10,
        "RandomSeed": 42
      }
    },
    "Metadata": {
      "OutputDirectory": "Metadata",
      "Providers": [ "json", "mermaid" ],
      "Json": {
        "WriteIndented": true
      },
      "Mermaid": {
        "Direction": "LR"
      }
    },
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Flowthru": "Debug",
        "Microsoft": "Warning"
      }
    }
  }
}
